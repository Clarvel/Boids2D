class Rectangle extends Renderable{
  float wide;
  float high;
  
  Rectangle(PVector pos, float wide, float high, color col){
    super(pos, col);
    this.wide = wide;
    this.high = high;
  }
  
  void render(){
    fill(this.col);
    noStroke();
    rect(this.pos.x, this.pos.y, wide, high);
  }
}
