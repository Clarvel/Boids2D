

class grouping {
  
ArrayList<Renderable> shapes;  
  
  grouping(ArrayList<Renderable> shapes)
  {  
    this.shapes = new ArrayList<Renderable>();
    this.shapes = shapes;
  }
  
  void render()
  {
    for(int i = 0 ; i < shapes.size() ;i++ )
    {
      shapes.get(i).render();
      
  }



}